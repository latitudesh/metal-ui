{
  "presets": ["@babel/env", "@babel/preset-react"],
  "plugins": [
    "babel-plugin-macros",
    "@babel/plugin-proposal-class-properties",
    [
      "@emotion/babel-plugin-jsx-pragmatic",
      {
        "export": "jsx",
        "import": "__cssprop",
        "module": "@emotion/react"
      }
    ],
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "__cssprop"
      }
    ]
  ]
}
